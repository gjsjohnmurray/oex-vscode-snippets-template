		"description": "Add inline TODO comment",
		"body": " /* TODO $0 */",
	},
	"TODO block": {
		"scope": "objectscript,objectscript-class,objectscript-int",
		"prefix": "TODO*",
		"description": "Add TODO comment block",
		"body": [
			" /* TODO",
			"$0",
			"*/"
		]
	},
	"TODO block + label": {
		"scope": "objectscript,objectscript-class,objectscript-int",
